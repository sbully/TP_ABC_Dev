<Window x:Name="Map" x:Class="Start_WF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Start_WF"
        mc:Ignorable="d"
        Title="MainWindow" Height="520" Width="700" ResizeMode="NoResize" Loaded="Map_Loaded" Closing="Map_Closing">
    <Grid x:Name="grille" HorizontalAlignment="Left" Height="475" Margin="10,10,0,0" VerticalAlignment="Top" Width="680" Background="Black">
        <Image x:Name="tank" Margin="0,415,630,10" Source="tank.png" Height="50" Width="50" Stretch="Fill"/>
        <Image x:Name="invader" Margin="0,340,630,85" Source="inavders.png" Stretch="Fill" Width="50" Height="50"/>
        <Image x:Name="obus" Margin="23,385,652,50" Source="bullet.png" Stretch="Fill" Width="5" Height="20" />

    </Grid>
</Window>
